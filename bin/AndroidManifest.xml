    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/gludroid"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.glug.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".os_activity"
            android:label="OS" />
        <activity android:name=".show_info" 
            android:label="Info" />
        <activity android:name=".foss_activity"
            android:label="FOSS" />
        <activity android:name=".sfd_activity" />
        <activity android:name=".about_sfd" />
        <activity android:name="com.example.glug.company_activity" />
        <activity android:name="com.example.glug.develop_activity" />
        <activity android:name="com.example.glug.member_activity" />
        <activity android:name=".glug_activity" />
        <activity android:name=".events_activity" />
        
    </application>

</manifest>
